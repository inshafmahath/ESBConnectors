<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="rsvps_getRSVP"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>
            <!-- Required parameters -->
            <property name="rsvp_id" expression="json-eval($.rsvp_id)"/>
            <property name="access_token" expression="json-eval($.access_token)"/>
            <!--Optional parameters -->
            <property name="fields" expression="json-eval($.fields)"/>
            <property name="REQUEST_HOST_HEADER" value="api.meetup.com" scope="axis2"/>
            <meetup.getRSVP>
                <rsvp_id>{$ctx:rsvp_id}</rsvp_id>
                <access_token>{$ctx:access_token}</access_token>
                <fields>{$ctx:fields}</fields>

            </meetup.getRSVP>
            <respond/>
        </inSequence>
        <outSequence>
            <log/>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>