<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="photos_getPhotoComments"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>
            <property name="photo_id" expression="json-eval($.photo_id)"/>
            <property name="key" expression="json-eval($.key)"/>
            <property name="fields" expression="json-eval($.fields)"/>
            <property name="member_id" expression="json-eval($.member_id)"/>
            <property name="name" expression="json-eval($.name)"/>
            <property name="time" expression="json-eval($.time)"/>
            <property name="REQUEST_HOST_HEADER" value="api.meetup.com" scope="axis2"/>
            <meetup.getPhotoComments>
                <photo_id>{$ctx:photo_id}</photo_id>
                <key>{$ctx:key}</key>
                <fields>{$ctx:fields}</fields>
                <member_id>{$ctx:member_id}</member_id>
                <name>{$ctx:name}</name>
                <time>{$ctx:time}</time>
            </meetup.getPhotoComments>
            <respond/>
        </inSequence>
        <outSequence>
            <log/>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>