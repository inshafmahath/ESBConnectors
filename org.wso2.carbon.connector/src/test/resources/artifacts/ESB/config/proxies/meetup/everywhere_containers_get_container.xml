<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="everywhere_containers_get_container"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>
            <property name="REQUEST_HOST_HEADER" value="api.meetup.com" scope="axis2"/>
            <property name="key" expression="json-eval($.key)"/>
            <property name="container_id" expression="json-eval($.container_id)"/>
            <property name="link" expression="json-eval($.link)"/>
            <property name="urlname" expression="json-eval($.group_id)"/>
             <property name="fields" expression="json-eval($.fields)"/>
            <meetup.init/>
            <meetup.getContainer>
                <key>{$ctx:key}</key>
                <container_id>{$ctx:container_id}</container_id>
                <link>{$ctx:link}</link>
                <urlname>{$ctx:urlname}</urlname>
                <fields>{$ctx:fields}</fields>
            </meetup.getContainer>
            <respond/>
        </inSequence>
        <outSequence>

            <send/>
        </outSequence>
    </target>
    <parameter name="serviceType">proxy</parameter>
    <description/>
</proxy>
                                