<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="oembed_getOEmbed"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>
            <property name="url" expression="json-eval($.url)"/>
            <property name="REQUEST_HOST_HEADER" value="api.meetup.com" scope="axis2"/>
            <meetup.getOEmbed>
                <url>{$ctx:url}</url>
            </meetup.getOEmbed>
            <respond/>
        </inSequence>
        <outSequence>
            <log/>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>