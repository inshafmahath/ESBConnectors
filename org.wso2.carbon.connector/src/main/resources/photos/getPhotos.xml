<?xml version="1.0" encoding="UTF-8"?>
<template name="getPhotoAlbums" xmlns="http://ws.apache.org/ns/synapse" description="This method returns photos by member, group, album, event, photo ID, or tagged member.">

    <!-- Required parameters -->
    <parameter name="event_id" description="event_id "/>
    <parameter name="key" description="API key"/>

    <sequence>
        <property name="uri.var.event_id" expression="$func:event_id"/>
        <property name="uri.var.key" expression="$func:key"/>
        <call>
            <endpoint>
                <http method="get" uri-template="https://api.meetup.com/2/photos?event_id={uri.var.event_id}&amp;key={uri.var.key}" />

            </endpoint>
        </call>

        <!-- Remove response custom header information -->
        <header name="x-li-format" scope="transport" action="remove" />
        <header name="X-LI-UUID" scope="transport" action="remove" />
        <header name="X-Li-Pop" scope="transport" action="remove" />
        <header name="X-Li-Fabric" scope="transport" action="remove" />
        <header name="x-li-request-id" scope="transport" action="remove" />

    </sequence>
</template>