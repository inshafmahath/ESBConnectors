<component name="notifications" type="synapse/template">
    <subComponents>
        <component name="getNotifications">
            <file>getNotifications.xml</file>
            <description>Returns all recent Meetup notifications for the authorized member.
            </description>
        </component>
        <component name="getSettings">
            <file>getSettings.xml</file>
            <description>Lists group and member-specific notification settings for the authenticated
                member
            </description>
        </component>
        <component name="markRead">
            <file>markRead.xml</file>
            <description>Marks groups of notifications as read.</description>
        </component>
    </subComponents>
</component>